{
  "name": "puppet-master",
  "version": "1.0.0",
  "description": "智能自动化交互框架，用于批量处理加密钱包操作",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:server": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "dev:client": "vite",
    "build": "tsc && vite build",
    "build:server": "tsc",
    "test": "jest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@chakra-ui/react": "^2.10.7",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "antd": "^5.24.6",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "framer-motion": "^11.18.2",
    "openai": "^4.28.4",
    "playwright": "^1.42.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "styled-components": "^6.1.17",
    "typescript": "^5.4.2",
    "uuid": "^11.1.0",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "vite": "^6.2.5"
  },
  "directories": {
    "test": "tests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
